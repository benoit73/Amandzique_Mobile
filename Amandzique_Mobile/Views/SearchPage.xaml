        <SearchBar Placeholder="Search music" x:Name="musiqueSearchBar" Text="{Binding MusiqueSearched, Mode=TwoWay}"
                   ></SearchBar>
        <CollectionView x:Name="MusiquesCollectionView" ItemsSource="{Binding MusiquesCollection}">
            
        </CollectionView>
    </VerticalStackLayout>
</ContentView>
